{
    "name": "elite/strowallet-sdk",
    "description": "A simple SDK for making payments and creating USD virtual cards via the Strowallet API.",
    "type": "library",
    "require": {
        "illuminate/support": "^12.10",
        "guzzlehttp/guzzle": "^7.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elite\\StrowalletLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "elite",
            "email": "etech.io02@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Elite\\StrowalletLaravel\\StrowalletServiceProvider"
            ],
            "aliases": {
                "Strowallet": "Elite\\StrowalletLaravel\\StrowalletFacade"
            }
        }
    }
}
